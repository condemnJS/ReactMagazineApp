.preventDefault();\r\n        await dispatch({type: FETCH_AUTH_USER, params: user});\r\n    }\r\n\r\n    return (\r\n        <div className=\"login__wrapper\">\r\n            <form className=\"form__control\">\r\n                <img src={Logo} alt=\"\"/>\r\n                <h3>Войти в ТатарМаркет</h3>\r\n                <Input label=\"Введите почту\" type=\"email\" placeholder=\"qwe123@mail.ru\" error={errors.body?.email}  name=\"email\" setState={(target: string, value: string) => setUser({...user, [target]: value})}/> \r\n                <Input label=\"Введите пароль\" type=\"password\" placeholder=\"Tatar228\" error={errors.body?.password} setState={(target: string, value: string) => setUser({...user, [target]: value})} name=\"password\"/> \r\n                {errors.code === 403 && <span className=\"invalid_feedback mb-2\">{ errors.message }</span>}\r\n                <Button value=\"Авторизироватся\" className=\"auth_btn\" onClick={auth}/> \r\n                <Button value=\"Зарегистрироваться\" className=\"reg_btn\" onClick={() => history.push('/register')}/>\r\n            </form>\r\n        </div>\r\n    );\r\n}","D:\\Projects\\react-magazine\\src\\pages\\RegisterPage.tsx",["307","308","309"],"import React, {useEffect, useState} from 'react';\r\nimport {Input} from \"../components/FormComponents/Input\";\r\nimport Logo from \"../assets/logo.png\";\r\nimport {Button} from \"../components/FormComponents/Button\";\r\nimport {useDispatch} from \"react-redux\";\r\nimport { CLEAN_ERRORS, REGISTER_USER_FETCH } from '../redux/types';\r\nimport { useTypedSelector } from '../hooks/useTypedSelector';\r\nimport {useHistory, Link} from \"react-router-dom\";\r\n\r\nexport const RegisterPage:React.FC = () => {\r\n    const history = useHistory();\r\n    const dispatch = useDispatch();\r\n    const {errors} = useTypedSelector(state => state.errors);\r\n    const {user_id} = useTypedSelector(state => state.user_id);\r\n    const [user, setUser] = useState({email: '', password: '', tel: '', fio: '', password_confirmation: ''});\r\n\r\n    useEffect(() => {\r\n        dispatch({type: CLEAN_ERRORS});\r\n    }, []);\r\n\r\n    const register = async (event: MouseEvent) => {\r\n        event.preventDefault();\r\n        dispatch({type: CLEAN_ERRORS});\r\n        dispatch({type: REGISTER_USER_FETCH, params: user});\r\n        let promise = new Promise((resolve) => {\r\n            setTimeout(() => {\r\n                resolve(user_id)\r\n            }, 800);\r\n        }).then(res => {\r\n            if(Object.keys(errors).length === 0) {\r\n                history.push('/login');\r\n            }\r\n        })\r\n    }\r\n\r\n    return (\r\n        <div className=\"login__wrapper\">\r\n            <form className=\"form__control\">\r\n                <img src={Logo} alt=\"\"/>\r\n                <h3 className=\"title_register\">Зарегистрироваться в ТатарМаркет</h3>\r\n                <Input label=\"ФИО\" type=\"text\" placeholder=\"Гизатулин Султан Камильевич\" error={errors.body?.fio}  name=\"fio\" setState={(target: string, value: string) => setUser({...user, [target]: value})}/> \r\n                <Input label=\"Email\" type=\"email\" placeholder=\"qwe123@mail.ru\" error={errors.body?.email}  name=\"email\" setState={(target: string, value: string) => setUser({...user, [target]: value})}/> \r\n                <Input label=\"Телефон\" type=\"tel\" placeholder=\"89525552525\" error={errors.body?.tel}  name=\"tel\" setState={(target: string, value: string) => setUser({...user, [target]: value})}/> \r\n                <Input label=\"Введите пароль\" type=\"password\" placeholder=\"Tatar228\" error={errors.body?.password} setState={(target: string, value: string) => setUser({...user, [target]: value})} name=\"password\"/> \r\n                <Input label=\"Подтвердите пароль\" type=\"password\" placeholder=\"Tatar228\" error={errors.body?.password_confirmation} setState={(target: string, value: string) => setUser({...user, [target]: value})} name=\"password_confirmation\"/> \r\n                <Button value=\"Зарегистрироваться\" className=\"auth_btn\" onClick={register}/> \r\n                <Button value=\"Авторизироваться\" className=\"reg_btn\" onClick={() => history.push('/login')}/>\r\n            </form>\r\n        </div>\r\n    );\r\n}","D:\\Projects\\react-magazine\\src\\components\\FormComponents\\Input.tsx",[],"D:\\Projects\\react-magazine\\src\\components\\FormComponents\\Button.tsx",[],"D:\\Projects\\react-magazine\\src\\redux\\sagas\\usersSaga.ts",["310"],"import {put, call, takeEvery} from 'redux-saga/effects'\r\nimport {requestAuth} from \"../Api\";\r\nimport { FETCH_AUTH_USER, SET_TOKEN} from '../types';\r\nimport {SET_ERRORS} from \"../types\";\r\nimport {userDataI} from \"../Api\";\r\n\r\ninterface DataProps {\r\n    type: string,\r\n    params: any\r\n}\r\n\r\nfunction* userWorker({params}:DataProps): Generator {\r\n    try {\r\n        const data: any = yield call(requestAuth, params)\r\n        yield put({type: SET_TOKEN, token: data.data.body.token})\r\n    } catch(e) {\r\n        if(e.response.status === 422) {\r\n            yield put({type: SET_ERRORS, errors: e.response.data})\r\n        }\r\n        if(e.response.status === 403) {\r\n            yield put({type: SET_ERRORS, errors: e.response.data})\r\n        }\r\n    }\r\n}\r\n\r\n\r\nexport function* userWatcher() {\r\n    yield takeEvery(FETCH_AUTH_USER, userWorker);\r\n}\r\n\r\n\r\n","D:\\Projects\\react-magazine\\src\\redux\\reducers\\userReducer.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\Store.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\sagas\\registerSaga.ts",[],"D:\\Projects\\react-magazine\\src\\pages\\AdminPage.tsx",["311"],"import React from \"react\";\r\nimport {SideBar} from \"../components/SideBar\";\r\n\r\nexport const AdminPage = () => {\r\n    return (\r\n        <div className=\"mainAdmin\">\r\n            <div className=\"admin__head\">\r\n                <h2>Добро пожаловать в Админ панель