r\n            headerLayoutWrapper.current.classList.toggle('active__layout');\r\n        }\r\n    }\r\n\r\n    const scrollAnimatedChangeData = (event: React.MouseEvent<HTMLAnchorElement>) => {\r\n        let target = event.target as HTMLAnchorElement;\r\n        let targetElement;\r\n        if(target.href){\r\n            targetElement = target;\r\n        }else {\r\n            targetElement = target.parentElement as HTMLAnchorElement;\r\n        }\r\n        const slugRef = targetElement.href.split('/')[3]; // Slug\r\n        setSlugRef(slugRef);\r\n    }\r\n\r\n    return (\r\n        <Fragment>\r\n            <TopAdvertisingComponent/>\r\n            <MainHeader click={toggleBtnCatalog}/>\r\n            <RetractablePanel headerLayoutWrapper={headerLayoutWrapper} categories={categories} subcategories={subcategories} scrollAnimatedChangeData={scrollAnimatedChangeData} slugRef={slugRef}/>\r\n            <SideHeaderCategories categories={categories}/>\r\n        </Fragment>\r\n    )\r\n}\r\n\r\n\r\nexport default Header\r\n","D:\\Projects\\react-magazine\\src\\redux\\store\\store.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\sagas\\categoriesSaga.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\reducers\\rootReducer.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\types.ts",[],"D:\\Projects\\react-magazine\\src\\rootConstant.ts",[],"D:\\Projects\\react-magazine\\src\\hooks\\useTypedSelector.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\sagas\\index.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\Api.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\reducers\\categoriesReducer.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\reducers\\subcategoriesReducer.ts",[],"D:\\Projects\\react-magazine\\src\\redux\\sagas\\subcategoriesSaga.ts",[],"D:\\Projects\\react-magazine\\src\\components\\HeaderComponents\\MainHeader.tsx",[],"D:\\Projects\\react-magazine\\src\\components\\HeaderComponents\\RetractablePanel.tsx",["299"],"import React from \"react\";\r\nimpor